# API Keys
vkai.api_key=your_api_key_here
vkai.base_url=https://llm-proxy.vkteam.ru
vkai.model=deepseek-chat

# Ollama Configuration
ollama.base_url=http://localhost:11434
ollama.model=llama2

# RAG Configuration
rag.embedding_model=all-minilm-l6-v2
rag.vector_db_path=./data/vector_db
rag.index_path=./data/index

# Figma Configuration (опционально)
# Получите API ключ на https://www.figma.com/developers/api#access-tokens
figma.api_key=your_figma_api_key_here

# Atlassian Jira Configuration
# Для Server/Data Center используйте Personal Access Token
jira.url=https://jira.vk.team
jira.personal_token=your_jira_personal_token_here
# Для Cloud используйте username + api_token вместо personal_token
# jira.username=your.email@company.com
# jira.api_token=your_jira_api_token_here

# Atlassian Confluence Configuration (опционально, для будущего использования)
# confluence.url=https://confluence.vk.team
# confluence.personal_token=your_confluence_personal_token_here
# confluence.username=your.email@company.com
# confluence.api_token=your_confluence_api_token_here

# Application Settings
app.log_level=INFO
app.data_dir=./data
app.work_dir=.
